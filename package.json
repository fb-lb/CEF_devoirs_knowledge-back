{
  "name": "knowledge-back",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "cross-env DEBUG=knowledge-back:* env-cmd -f ./env/.env tsx --inspect=9229 --watch ./src/bin/www.ts",
    "build": "tsc",
    "start": "node ./dist/bin/www",
    "migrate": "cross-env env-cmd -f ./env/.env npx sequelize-cli db:migrate",
    "migrate-undo": "cross-env env-cmd -f ./env/.env npx sequelize-cli db:migrate:undo",
    "seeders": "cross-env env-cmd -f ./env/.env npx sequelize-cli db:seed:all"
  },
  "dependencies": {
    "@emailjs/nodejs": "^5.0.2",
    "bcrypt": "^6.0.0",
    "cookie-parser": "~1.4.7",
    "cors": "^2.8.5",
    "debug": "~4.4.1",
    "express": "~5.1.0",
    "morgan": "~1.10.1",
    "mysql2": "^3.14.5",
    "sequelize": "^6.37.7",
    "sequelize-cli": "^6.6.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/cookie-parser": "^1.4.9",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/morgan": "^1.9.10",
    "@types/node": "^24.4.0",
    "cross-env": "^10.0.0",
    "env-cmd": "^11.0.0",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2"
  }
}
